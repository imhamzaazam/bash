                            if (lrec3Div) {
                                if (lrec3Div.className.indexOf('lrec-bgcolor') < 0) {
                                    lrec3Div.className += ' lrec-bgcolor';
                                }
                                lrec3Div.className = lrec3Div.className.replace('D-n', '');
                                lrec3Div.className = lrec3Div.className.replace('Ht-300x600', '');
